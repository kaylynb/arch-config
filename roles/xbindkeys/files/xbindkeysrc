"amixer set Master playback 5%+"
  XF86AudioRaiseVolume

"amixer set Master playback 5%-"
  XF86AudioLowerVolume

"amixer set Master toggle"
  XF86AudioMute

"xbacklight -dec 5"
  XF86MonBrightnessDown

"xbacklight -inc 5"
  XF86MonBrightnessUp

"kbdlight up"
  XF86KbdBrightnessUp

"kbdlight down"
  XF86KbdBrightnessDown

"togglecapsctrl"
  Shift + Shift_R

"togglecapsctrl"
  Shift + Shift_L
